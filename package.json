{
  "name": "lubimovka_frontend",
  "version": "1.0.0",
  "description": "Фронтенд проекта «Любимовка»",
  "main": "index.js",
  "scripts": {
    "postinstall": "next telemetry disable",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint:js": "next lint",
    "lint:css": "stylelint **/*.css --cache --allow-empty-input --fix",
    "type-check": "node bin/type-check.js",
    "storybook": "start-storybook -s public -p 3001",
    "build-storybook": "build-storybook -s public",
    "update-typings": "rm -rf ./src/api-typings && npx openapi-typescript-codegen --input https://stage.dev.lubimovka.ru/api/v1/schema --output ./src/api-typings --useUnionTypes --exportCore false --exportServices false",
    "update-git-hooks": "npx simple-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Studio-Yandex-Practicum/lubimovka_frontend.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Studio-Yandex-Practicum/lubimovka_frontend/issues"
  },
  "homepage": "https://github.com/Studio-Yandex-Practicum/lubimovka_frontend#readme",
  "dependencies": {
    "@funboxteam/diamonds": "8.2.1",
    "classnames": "2.3.1",
    "express": "4.17.2",
    "http-proxy-middleware": "2.0.1",
    "keen-slider": "5.5.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-focus-lock": "2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-links": "6.3.8",
    "@storybook/react": "6.3.8",
    "@svgr/webpack": "5.5.0",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "babel-loader": "8.2.2",
    "chalk": "4.1.2",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-import": "2.25.3",
    "fetch-mock": "9.11.0",
    "lint-staged": "11.1.2",
    "postcss": "8.3.9",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-functions": "4.0.2",
    "postcss-import": "14.0.2",
    "postcss-mixins": "8.1.0",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "6.7.0",
    "postcss-simple-vars": "6.0.3",
    "simple-git-hooks": "2.6.1",
    "stylelint": "13.13.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "22.0.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run type-check"
  }
}
